{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run",
			"preLaunchTask": "build",
			"internalConsoleOptions": "openOnSessionStart",
			"request": "launch",
			"type": "coreclr",
			"cwd": "${workspaceFolder}/Web/UserInterface",
			"program": "${workspaceFolder}/Web/UserInterface/bin/Debug/netcoreapp2.1/Solution.Web.UserInterface.dll",
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"launchBrowser": {
				"args": "${auto-detect-url}",
				"enabled": true,
				"windows": {
					"command": "cmd.exe",
					"args": "/C start ${auto-detect-url}"
				}
			}
		}
	]
}
